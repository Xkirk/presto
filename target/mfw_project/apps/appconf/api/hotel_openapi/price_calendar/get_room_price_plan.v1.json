{
  "title": "获取房型价格计划简要信息",
  "login": true,
  "method": "GET",
  "input": {
    "type": "object",
    "properties": {
      "room_id": {
        "type": "integer"
      },
      "month_list":{
        "type": "string",
        "description": "需要的月份，如201709。逗号分隔"
      }
    },
    "required": [
      "room_id",
      "month_list"
    ]
  },
  "output": [
    {
      "code": 0,
      "message": "",
      "data": {
        "type": "object",
        "properties": {
          "list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "date": {
                  "type": "number"
                },
                "date_str": {
                  "type": "string"
                },
                "sale_num": {
                  "type": "integer"
                },
                "room_num": {
                  "type": "integer"
                },
                "price": {
                  "type": "integer"
                },
                "added_last": {
                  "type": "integer"
                },
                "can_book": {
                  "type": "integer"
                },
                "has_breakfast": {
                  "type": "integer"
                },
                "cancel_type": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    }
  ]
}