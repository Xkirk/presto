{
  "title": "获取所有房型当月价格计划简要信息",
  "login": true,
  "method": "GET",
  "input": {
    "type": "object",
    "properties": {
      "month":{
        "type": "string",
        "description": "需要的月份，如201709"
      },
      "hotel_id": {
        "type": "integer"
      }
    },
    "required": [
      "month"
    ]
  },
  "output": [
    {
      "code": 0,
      "message": "",
      "data": {
        "type": "object",
        "properties": {
          "list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "name": {
                  "type": "string"
                },
                "price_plan": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "date": {
                        "type": "number"
                      },
                      "status": {
                        "type": "integer",
                        "description": "1：有预定，2：不可预定"
                      },
                      "date_str": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  ]
}