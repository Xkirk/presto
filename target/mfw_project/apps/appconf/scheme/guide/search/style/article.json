{
  "type": "object",
  "description": "自由行攻略文章style",
  "properties": {
    "style": {
      "type": "string",
      "enum": [
        "article"
      ],
      "description": "客户端的呈现样式style"
    },
    "data": {
      "type": "object",
      "properties": {
        "id": {
           "type": "string",
           "description":"id"
        },
        "title": {
          "type": "string",
          "description": "文章标题"
        },
        "thumbnail": {
          "type": "string",
          "description": "缩略图"
        },
        "content": {
          "type": "string",
          "description": "文章内容描述"
        },
        "result_type": {
          "type":"string",
          "description":"内容类型"
        },
        "jump_url":{
          "type":"string",
          "description":"跳转链接"
        },
        "bottom": {
          "type": "object",
          "properties":{
            "desc_text":{
              "type":"string",
              "description":"左下角描述文案，例如:目的地/日期"
            },
            "attach_text":{
              "type":"string",
              "description":"右下角描述文案，例如:***浏览"
            }
          }
        }
      }
    }
  }
}