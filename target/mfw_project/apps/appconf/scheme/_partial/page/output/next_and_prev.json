{
  "type": "object",
  "description": "某数据片段的返回结构，包括了上下边界",
  "properties": {
    "next": {
      "type": "boolean",
      "description": "是否有下一页"
    },
    "next_boundary": {
      "type": "string",
      "description": "返回的数据片段的下边界"
    },
    "prev": {
      "type": "boolean",
      "description": "是否有上一页"
    },
    "prev_boundary": {
      "type": "string",
      "description": "返回的数据片段的上边界"
    }
  },
  "allowAdditional": false,
  "required": ["next","next_boundary","prev","prev_boundary"]
}