{
  "description": "向上翻页的返回结构；向下翻页的返回结构",
  "anyOf": [
    {
      "description": "向下翻页的返回结构",
      "ref": "_partial/page/output/next"
    },
    {
      "type": "object",
      "description": "向上翻页返回结构",
      "properties": {
        "prev": {
          "type": "boolean",
          "description": "是否有上一页"
        },
        "prev_boundary": {
          "type": "string",
          "description": "返回的数据片段的上边界"
        }
      },
      "allowAdditional": false,
      "required": ["prev","prev_boundary"]
    }
  ]
}