{
  "type": "object",
  "description": "直播间实体",
  "properties": {
    "id": {
      "type": "string",
      "description": "直播间号,唯一标示"
    },
    "title": {
      "type": "string",
      "description": "直播间名称"
    },
    "num_watching": {
      "type": "integer",
      "description": "观众人数"
    },
    "is_happening": {
      "type": "boolean",
      "description": "是否正在直播"
    },
    "host": {
      "ref": "user/user",
      "description": "主播信息"
    },
    "watch_key": {
      "type": "string",
      "description": "直播的观看地址"
    },
    "push_key": {
      "type": "string",
      "description": "推流的地址,只在请求用户为该房间主播时才会返回"
    },
    "cover_img": {
      "ref": "_partial/image",
      "include": ["simg", "bimg"]
    },
    "mdd": {
      "ref": "mdd/mdd",
      "include": ["id","name","parent"]
    },
    "watchers": {
      "type": "array",
      "description": "房间里的观众列表, 该数组最多返回10个就行",
      "items" : {
        "ref": "user/user",
        "include": ["id","name","logo"]
      }
    }
  }
}