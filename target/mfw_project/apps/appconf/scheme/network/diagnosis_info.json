{
  "type": "object",
  "description": "网络连接诊断结果model",
  "properties": {
    "domain": {
      "type": "string",
      "description": "域名"
    },
    "net_status": {
      "type": "integer",
      "description": "联网状态，0是未连接，1是已连接"
    },
    "network_type": {
      "type": "string",
      "description": "网络类型，WIFI/3G/4G..."
    },
    "local_ip": {
      "type": "string",
      "description": "本地ip"
    },
    "local_gateway": {
      "type": "string",
      "description": "本地网关"
    },
    "local_dns": {
      "type": "string",
      "description": "本地dns"
    },
    "domain_ip": {
      "type": "string",
      "description": "域名ip解析结果"
    },
    "telnet_info": {
      "type": "string",
      "description": "tcp连接信息"
    },
    "ping_info": {
      "type": "string",
      "description": "ping结果信息"
    },
    "trace_route_info": {
      "type": "string",
      "description": "traceRoute过程信息"
    },
    "public_ip": {
      "type": "string",
      "description": "公网ip"
    },
    "timestamp": {
      "type": "integer",
      "description": "诊断时间,单位秒"
    },
    "device_id": {
      "type": "string",
      "description": "设备标识"
    },
    "uid": {
      "type": "string",
      "description": "用户id，可为空"
    },
    "hardware_model": {
      "type": "string",
      "description": "设备名"
    },
    "system_version": {
      "type": "string",
      "description": "系统版本"
    },
    "app_version": {
      "type": "string",
      "description": "app版本，versionName和code的组合"
    },
    "app_code": {
      "type": "string",
      "description": "ios or android的包名"
    }
  }
}