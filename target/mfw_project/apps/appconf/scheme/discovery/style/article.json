{
    "type": "object",
    "description": "大图文章",
    "properties": {
        "style": {
            "type": "string",
            "enum": [
                "article"
            ]
        },
        "data": {
            "type" : "object",
            "properties": {
                "abtest": {
                    "type": "string",
                    "description": "ab测试用，默认为空"
                },
                "image": {
                    "type": "string"
                },
                "width":{
                    "type":"integer",
                    "description":"图片宽度(dp)"
                },
                "height":{
                    "type":"integer",
                    "description":"图片高度(dp)"
                },
                "jump_url": {
                    "type": "string",
                    "description": "跳转链接"
                },
                "business_id": {
                    "type": "string",
                    "description": "业务id，上报日志用"
                },
                "item_type": {
                    "type": "string",
                    "description": "业务类型，上报日志用"
                },
                "title": {
                    "type": "string",
                    "description": "标题"
                },
                "badge": {
                    "ref": "_partial/badge"
                },
                "bottom": {
                    "ref": "discovery/bottom",
                    "additional": {
                        "user_list": {
                            "type": "array",
                            "items": {
                                "ref": "user/user",
                                "include": [
                                    "logo",
                                    "id"
                                ],
                                "additional": {
                                    "jump_url": {
                                        "type": "string",
                                        "description": "跳转链接"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}