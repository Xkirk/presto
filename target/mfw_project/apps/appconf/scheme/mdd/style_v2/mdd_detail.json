{
    "type": "object",
    "description": "目的地详情,目的地页",
    "properties": {
        "id": {
            "type": "string",
            "description": "目的地id"
        },
        "name": {
            "type": "string",
            "description": "目的地名"
        },
        "lat": {
            "type": "number",
            "description": "纬度"
        },
        "lng": {
            "type": "number",
            "description": "经度"
        },
        "map_provider": {
            "type": "string",
            "description": "地图提供商",
            "enum": [
                "amap",
                "baidu",
                "google"
            ]
        },
        "is_country": {
            "type": "integer",
            "description": "是否国际级目的地 0 不是 1 是",
            "enum": [
                0,
                1
            ]
        },
        "is_fav": {
            "type": "boolean",
            "description": "是否收藏"
        },
        "header_img": {
            "type": "string",
            "description": "头图"
        },
        "parent": {
            "type": "object",
            "description": "父级目的地",
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "目的地id"
                },
                "name": {
                    "type": "string",
                    "description": "目的地名称"
                }
            }
        }
    }
}