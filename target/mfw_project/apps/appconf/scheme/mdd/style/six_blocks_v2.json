{
  "type": "object", 
  "description": "目的地页极速预定入口", 
  "properties": {
    "style": {
      "type": "string", 
      "enum": [
        "six_blocks_v2"
      ],
      "description":"android7.9.3 ios7.9.4以上用此样式 老版本用six_blocks"
    }, 
    "type": {
      "type": "string", 
      "description": "对应的业务类型,极速预定billions_board"
    }, 
    "data": {
      "type": "object", 
      "properties": {
        "image": {
          "type": "object", 
          "description": "玩／住的图片", 
          "properties": {
            "url": {
              "type": "string"
            }, 
            "width": {
              "type": "integer"
            }, 
            "height": {
              "type": "integer"
            }
          }
        },
        "line_color":{
            "type":"string",
            "description":"线的颜色，请务必返回#8位 如#00000000"
        },
        "list": {
          "type": "array", 
          "items": {
            "type": "object", 
            "properties": {
              "title": {
                "type": "string", 
                "description": "方块中的主标题"
              }, 
              "sub_title": {
                "type": "string", 
                "description": "方块中的副标题"
              }, 
              "business_type": {
                "type": "string", 
                "enum": [
                  "sales", 
                  "hotel"
                ], 
                "description": "业务类型，sales 自由行商品 hotel 酒店"
              }, 
              "icon": {
                "type": "string", 
                "description": "标签url"
              }, 
              "jump_url": {
                "type": "string", 
                "description": "跳转链接"
              }
            }
          }
        }
      }
    }
  }
}