{
  "type": "object",
  "description": "带标签的自由行商品",
  "properties": {
    "style": {
      "type": "string",
      "enum": [
        "sales_with_theme_v2"
      ],
      "description": "客户端的呈现样式"
    },
    "data": {
      "type": "object",
      "description": "数据",
      "properties": {
        "title": {
          "type": "string",
          "description": "标题"
        },
        "jump_url": {
          "type": "string",
          "description": "跳转链接"
        },
        "bottom": {
          "type": "string",
          "description": "查看更多 这样的文字"
        },
        "has_more": {
          "type": "boolean",
          "description": "true 显示bottom"
        },
        "top_banner": {
          "type": "object",
          "description": "顶部主题位",
          "properties": {
            "width": {
              "type": "integer",
              "description": "宽 dp"
            },
            "height": {
              "type": "integer",
              "description": "高 dp"
            },
            "thumbnail": {
              "type": "integer",
              "description": "主题图片地址"
            },
            "jump_url": {
              "type": "integer",
              "description": "跳转链接"
            }
          }
        },
        "bottom_banner": {
          "type": "object",
          "description": "末尾主题位",
          "properties": {
            "width": {
              "type": "integer",
              "description": "宽 dp"
            },
            "height": {
              "type": "integer",
              "description": "高 dp"
            },
            "thumbnail": {
              "type": "integer",
              "description": "主题图片地址"
            },
            "jump_url": {
              "type": "integer",
              "description": "跳转链接"
            }
          }
        },
        "item_bg_color": {
          "type": "string",
          "description": "主题背景色, 类似：#FED24B"
        },
        "list": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "tag_name": {
                "type": "sting",
                "description": "tag名称"
              },
              "jump_url": {
                "type": "sting",
                "description": "tag下的jump url"
              },
              "list": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "title": {
                      "type": "string"
                    },
                    "subtitle": {
                      "type": "string",
                      "description": "支持html,信息描述,例如:班期"
                    },
                    "bottom": {
                      "type": "string",
                      "description": "底部信息栏,例如：已售3"
                    },
                    "thumbnail": {
                      "type": "string"
                    },
                    "desc": {
                      "type": "string",
                      "description": "产品描述(左上角，如北京／北京出发)"
                    },
                    "type": {
                      "type": "string",
                      "description": "机+酒等文字"
                    },
                    "features":{
                      "type":"array",
                      "description":"商品特色",
                      "items":{
                        "type":"object",
                        "properties":{
                          "text":{
                            "type":"string",
                            "description":"特色描述"
                          },
                          "text_color":{
                            "type":"string",
                            "description":"文字颜色"
                          },
                          "bg_start_color":{
                            "type":"string",
                            "description":"文字背景开始颜色"
                          },
                          "bg_end_color":{
                            "type":"string",
                            "description":"文字背景结束颜色"
                          }
                        }
                      }
                    },
                    "price": {
                      "type": "object",
                      "description": "产品价格",
                      "properties": {
                        "number": {
                          "type": "string",
                          "description": "价格"
                        },
                        "type": {
                          "type": "string",
                          "description": "价格单位"
                        },
                        "suffix": {
                          "type": "string",
                          "description": "价格后缀，如：起"
                        }
                      }
                    },
                    "jump_url": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}