{
  "type": "object",
  "description": "带封面图的文章，可能带有标签，作者之类的",
  "properties": {
    "style": {
      "type": "string",
      "enum": [
        "travel_plan"
      ]
    },
    "data": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "description": "模块标题，如果为空则不显示，如果分页需要合并上一页相同模块"
        },
        "list": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string",
                "description": "标题"
              },
              "jump_url": {
                "type": "string",
                "description": "跳转链接"
              },
              "bottom": {
                "type": "string",
                "description": "查看更多 这样的文字"
              },
              "has_more": {
                "type": "boolean",
                "description": "true 显示bottom"
              },
              "list": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string"
                    },
                    "sub_title": {
                      "type": "string"
                    },
                    "img_url": {
                      "type": "string"
                    },
                    "jump_url": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}