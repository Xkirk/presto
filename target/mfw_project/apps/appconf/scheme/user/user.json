{
  "type": "object",
  "description": "用户基本实体",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "level": {
      "type": "integer",
      "description": "用户等级"
    },
    "logo": {
      "type": "string",
      "description": "用户头像url"
    },
    "gender": {
      "type": "integer",
      "description": "女0 男1 未知2"
    },
    "intro": {
      "type": "string",
      "description": "用户签名"
    },
    "about": {
      "type": "string",
      "description": "关于用户的描述,平台给予的"
    },
    "is_follow": {
      "type": "boolean",
      "description": "是否已关注该用户"
    },
    "num_fans": {
      "type": "integer",
      "description": "粉丝数"
    },
    "num_follows": {
      "type": "integer",
      "description": "关注他人数"
    },
    "status": {
      "type": "integer",
      "enum": [0, 1, 2, 3],
      "description": "用户身份枚举, normal = 0, VIP = 1 , Official = 2, Enterprise = 3"
    },
    "status_url": {
      "type": "string",
      "description": "用户头像上的身份类型的图片url, 例如认证过的大V 企业账号 官方账号等"
    },
    "badges": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "用户勋章或身份的名称,UI显示顺序由服务端排序控制, 例如指路人 蜂首等头衔文字"
      }
    },
    "level_progress": {
      "type": "integer",
      "description": "用户当前等级经验百分比"
    },
    "elapse_join_time": {
      "type": "integer",
      "description": "加入蚂蜂窝X天"
    },
    "next_level_exp": {
      "type": "integer",
      "description": "距离下一等级经验"
    }
  }
}