{
  "type": "object",
  "description": "游记类型model",
  "properties": {
    "id": {
      "type": "string",
      "description": "游记id"
    },
    "title": {
      "type": "string",
      "description": "游记标题"
    },
    "user": {
      "ref": "/user/user",
      "include": [
        "id",
        "logo",
        "name",
        "status",
        "status_url"
      ],
      "description": "游记作者对象"
    },
    "mdds": {
      "type": "array",
      "items": {
        "type": "object",
        "description": "",
        "ref": "/mdd/mdd",
        "include": [
          "id",
          "name"
        ]
      }
    },
    "jump_url": {
      "type": "string"
    },
    "num_vote": {
      "type": "integer",
      "description": "顶的数量"
    },
    "thumbnail": {
      "type": "string",
      "description": "游记图片"
    },
    "is_voted": {
      "type": "integer",
      "description": "是否顶过"
    },
    "ctime": {
      "type": "integer",
      "description": "创建时间"
    },
    "elite_time": {
      "type": "integer",
      "description": "蜂首时间"
    },
    "num_comment": {
      "type": "integer",
      "description": "评论数"
    },
    "num_visit": {
      "type": "integer",
      "description": "访问数"
    },
    "operate_tags": {
      "type": "array",
